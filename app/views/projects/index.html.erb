<%= link_to 'Home', "http://localhost:3000/" %>

<h1>All Projects</h1>

<ul>
  <%if @projects.length >0%>
    <% @projects.each do |project| %>
      <li><%= link_to project.name, project_path(project) %></li>
    <% end %>
  <%else%>
    <p>No Projects to show</p>
  <%end%>
</ul>

<%= link_to 'New Project', new_project_path %>